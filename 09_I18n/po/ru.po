#: src/predict_number.c:18
#, c-format
msgid "Please, enter any number from 1 to 100 included\n"
msgstr "Введите, пожалуйста, любое число от 1 до 100 включительно\n"

#: src/predict_number.c:22
msgid "yes"
msgstr "да"

#: src/predict_number.c:23
msgid "not"
msgstr "нет"

#: src/predict_number.c:26
#, c-format
msgid "Can I ask if your number greater than %d (write yes or not)?\n"
msgstr "Могу ли я спросить про ваше загаданное число: оно больше чем %d (напишите да или нет)\n"

#: src/predict_number.c:37
#, c-format
msgid "Error input, try again\n"
msgstr "Ошибка ввода, попробуйте еще раз\n"

#: src/predict_number.c:41
#, c-format
msgid "Your desired number was: %d\n"
msgstr "Ваше желанное число было %d\n"
